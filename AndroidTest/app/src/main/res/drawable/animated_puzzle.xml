<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/puzzle_button">
    <target android:name="red">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="2000"
                android:repeatCount="-1">
                <propertyValuesHolder android:propertyName="translateX">
                    <keyframe
                        android:fraction="0"
                        android:value="50" />
                    <keyframe
                        android:fraction=".25"
                        android:value="0" />
                </propertyValuesHolder>
                <propertyValuesHolder android:propertyName="translateY">
                    <keyframe
                        android:fraction="0"
                        android:value="40" />
                    <keyframe
                        android:fraction=".25"
                        android:value="0" />
                </propertyValuesHolder>
            </objectAnimator>

        </aapt:attr>
    </target>
    <target android:name="darkGreen">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="2000"

                android:repeatCount="-1">
                <propertyValuesHolder android:propertyName="rotation">
                    <keyframe
                        android:fraction="0"
                        android:value="30" />
                    <keyframe
                        android:fraction=".25"
                        android:value="30" />
                    <keyframe
                        android:fraction=".5"
                        android:value="0" />
                </propertyValuesHolder>
                <propertyValuesHolder android:propertyName="translateY">
                    <keyframe
                        android:fraction="0"
                        android:value="60" />
                    <keyframe
                        android:fraction=".25"
                        android:value="60" />
                    <keyframe
                        android:fraction=".5"
                        android:value="0" />
                </propertyValuesHolder>
                <propertyValuesHolder android:propertyName="translateX">
                    <keyframe
                        android:fraction="0"
                        android:value="50" />
                    <keyframe
                        android:fraction=".25"
                        android:value="50" />
                    <keyframe
                        android:fraction=".5"
                        android:value="0" />
                </propertyValuesHolder>
            </objectAnimator>
        </aapt:attr>
    </target>
    <target android:name="darkBlue">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="2000"
                android:repeatCount="-1">
                <propertyValuesHolder android:propertyName="rotation">
                    <keyframe
                        android:fraction="0"
                        android:value="45" />
                    <keyframe
                        android:fraction=".5"
                        android:value="45" />
                    <keyframe
                        android:fraction=".75"
                        android:value="0" />
                </propertyValuesHolder>
                <propertyValuesHolder android:propertyName="translateY">
                    <keyframe
                        android:fraction="0"
                        android:value="40" />
                    <keyframe
                        android:fraction=".5"
                        android:value="40" />
                    <keyframe
                        android:fraction=".75"
                        android:value="0" />
                </propertyValuesHolder>
                <propertyValuesHolder android:propertyName="translateX">
                    <keyframe
                        android:fraction="0"
                        android:value="150" />
                    <keyframe
                        android:fraction=".5"
                        android:value="150" />
                    <keyframe
                        android:fraction=".75"
                        android:value="0" />
                </propertyValuesHolder>
            </objectAnimator>
        </aapt:attr>
    </target>


</animated-vector>